{
  "name": "new-start-vue",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --mode development",
    "build": "run-p type-check build-only",
    "preview": "vite build && vite preview --port 8082",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "style:fix": "stylelint \"**/*.{vue,css,less,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@vueuse/shared": "^10.7.0",
    "http-server": "^14.1.1",
    "lru-cache": "^7.18.3",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qs": "^6.11.2",
    "vant": "^4.8.0",
    "vue": "^3.3.10",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@erhang/eslint-config": "^1.0.6",
    "@erhang/stylelint-config": "^1.0.6",
    "@faker-js/faker": "^8.3.1",
    "@intlify/unplugin-vue-i18n": "^0.10.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/etag": "^1.8.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^16.18.68",
    "@types/nprogress": "0.2.0",
    "@types/qs": "^6.9.10",
    "@vant/auto-import-resolver": "^1.0.2",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-macros/volar": "^0.10.2",
    "@vue/compiler-dom": "^3.3.11",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "axios-extensions": "^3.1.7",
    "chalk": "^4.1.2",
    "commitizen": "^4.3.0",
    "cors": "^2.8.5",
    "cz-git": "^1.8.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "etag": "^1.8.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "magic-string": "^0.29.0",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.1",
    "postcss-nesting": "^12.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "svg-baker": "^1.7.0",
    "svgo": "^2.8.0",
    "tailwindcss": "^3.3.6",
    "terser": "^5.26.0",
    "typeit": "^8.8.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.1",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.7.42",
    "vite-plugin-mock": "^2.9.8",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-static-copy": "^0.13.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.24.5",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix --allow-empty-input"
    ],
    "*.{html,sass,scss,less}": [
      "prettier --write",
      "stylelint --fix --allow-empty-input"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.15.1"
  }
}
